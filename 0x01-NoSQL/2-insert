// insert to collection
#!/usr/bin/env bash

if [ "$#" -gt 0 ]; then
    # Use the first argument as the database name
    db_name="$1"
    # Run the MongoDB command to insert the document
    mongo "$db_name" --eval 'db.school.insert({ name: "Holberton school" })'
else
    # Print usage message if no arguments are passed
    echo "Usage: $0 db_name"
fi
